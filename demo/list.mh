type Maybe = [A] Just(A) | Nothing
type List = [A] Cons(A, List(A)) | Nil

let intList = List(Int)

print(intList)

let #:# = (el, list) => Cons(el, list)
let #$# = (f, x) => f(x)
let #.# = (g, f) => (x) => f(g(x))

let get = (list, index) =>
  match list then
    | Cons(a, b) =>
      if index == 0 then Just(a) else get(b, index - 1)
    | Nil =>
      Nothing

let map = (f, list) =>
  match list then
    | Cons(a, b) =>
      Cons(f(a), map(f, b))
    | Nil => Nil

let #++# = (list1, list2) =>
  match list1 then
    | Cons(a, b) =>
      Cons(a, b ++ list2)
    | Nil => list2

let #+++# = (str1, str2) => concat(str1, str2)

let show = (x) =>
  let f = (x) =>
    match x then
      | Cons(a, b) =>
        a +++ let c = f(b) in if c == "" then "" else ", " +++ c
      | Nil => ""
  in "[" +++ f(x) +++ "]"

let arr = 1 : 2 : 3 : 4 : Nil

print(show $ arr)
print(map (print, get(arr, 2) : get(arr, 6) : Nil))
